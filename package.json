    "npm-install-peers": "^1.2.2",
    "prettier": "^3.0.2",
    "source-map-loader": "^4.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "5.2.1-rc",
    "webpack": "5.55.1",
    "webpack-cli": "^5.1.4",
