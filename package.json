{
  "name": "tool_platform",
  "version": "1.0.0",
  "description": "工具集合",
  "main": "index.ts",
  "scripts": {
    "prettier-lint": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "eslint": "eslint static --ext .ts --ext .tsx",
